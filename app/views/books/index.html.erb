<p id="notice"><%= notice %></p>

<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Link</th>
      <th>ISBN</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.link %></td>
        <td><%= book.isbn %></td>
        <td><%= link_to 'Show', book %></td>
        
        
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book', new_book_path %>
